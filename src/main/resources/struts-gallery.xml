<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- 图库 -->
	<package name="gallery" namespace="/gallery" extends="default">
		<action name="upload" class="UploadImageAction">
			<interceptor-ref name="defaultStack"></interceptor-ref>
		</action>
		<action name="upload-progress" class="UploadProgressAction">
			<interceptor-ref name="defaultStack"></interceptor-ref>
		</action>

		<action name="find" class="ImageAction"></action>
		
		<action name="list-oneself" class="GalleryAction" method="listOneself"></action>
	</package>

</struts>